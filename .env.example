# ===========================================
# GeoExplore-AI Environment Configuration
# ===========================================
# Copy this file to .env and fill in your keys

# --- OpenAI ---
OPENAI_API_KEY=sk-your-openai-api-key-here

# --- ArcGIS Developer Account ---
ARCGIS_API_KEY=your-arcgis-api-key-here
ARCGIS_CLIENT_ID=your-client-id
ARCGIS_CLIENT_SECRET=your-client-secret

# --- App Settings ---
APP_ENV=development
APP_DEBUG=true
APP_PORT=8000
FRONTEND_URL=http://localhost:5173

# --- Database (optional, for caching/saving tours) ---
DATABASE_URL=sqlite:///./geoexplore.db
